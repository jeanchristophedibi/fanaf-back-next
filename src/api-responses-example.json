    "schema_supabase_attendues": {
      "table_participants": {
        "columns": [
          "id (uuid, primary key)",
          "nom (varchar)",
          "prenom (varchar)",
          "reference (varchar, unique)",
          "email (varchar)",
          "telephone (varchar)",
          "pays (varchar)",
          "fonction (varchar, nullable)",
          "organisation_id (uuid, foreign key)",
          "statut (varchar: membre|non-membre|vip|speaker|referent)",
          "statut_inscription (varchar: finalisée|non-finalisée)",
          "date_inscription (timestamp)",
          "date_paiement (timestamp, nullable)",
          "mode_paiement (varchar, nullable)",
          "canal_encaissement (varchar, nullable)",
          "caissier (varchar, nullable)",
          "badge_genere (boolean, default false)",
          "check_in (boolean, default false)",
          "check_in_date (timestamp, nullable)",
          "groupe_id (uuid, nullable)",
          "nom_groupe (varchar, nullable)",
          "created_at (timestamp)",
          "updated_at (timestamp)"
        ]
      },
      "table_organisations": {
        "columns": [
          "id (uuid, primary key)",
          "nom (varchar)",
          "contact (varchar)",
          "email (varchar大型)",
          "pays (varchar)",
          "date_creation (date)",
          "statut (varchar: membre|non-membre|sponsor)",
          "secteur_activite (varchar, nullable)",
          "referent_nom (varchar, nullable)",
          "referent_prenom (varchar, nullable)",
          "referent_email (varchar, nullable)",
          "referent_telephone (varchar, nullable)",
          "referent_fonction (varchar, nullable)",
          "created_at (timestamp)",
          "updated_at (timestamp)"
        ]
      },
      "table_rendez_vous": {
        "columns": [
          "id (uuid, primary key)",
          "demandeur_id (uuid, foreign key)",
          "recepteur_id (uuid)",
          "type (varchar: participant|sponsor)",
          "date (date)",
          "heure (time)",
          "statut (varchar: acceptée|occupée|en-attente|annulée)",
          "commentaire (text, nullable)",
          "created_at (timestamp)",
          "updated_at (timestamp)"
        ]
      }
    },

    "mapping_noms_colonnes": {
      "note": "Si vos tables Supabase utilisent snake_case, ajoutez un mapping dans useApi.ts",
      "exemple": {
        "statut_inscription": "statutInscription",
        "date_inscription": "dateInscription",
        "date_paiement": "datePaiement",
        "mode_paiement": "modePaiement",
        "canal_encaissement": "canalEncaissement",
        "badge_genere": "badgeGenere",
        "check_in": "checkIn",
        "check_in_date": "checkInDate",
        "groupe_id": "groupeId",
        "nom_groupe": "nomGroupe"
      }
    }
