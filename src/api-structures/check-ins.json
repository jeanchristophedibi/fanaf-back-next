{
  "description": "Structure de données pour les check-ins - API Supabase",
  "table": "check_ins",
  "schema": {
    "id": "string (UUID)",
    "participant_id": "UUID (foreign key -> participants)",
    "date_check_in": "date",
    "heure_check_in": "time",
    "scan_par": "UUID (nullable - foreign key -> membres_comite)",
    "autorise": "boolean",
    "raison_refus": "string (nullable - si autorise = false)",
    "nombre_scans": "integer",
    "statut_remontee": "enum('signale', 'normal') (nullable)",
    "created_at": "timestamp",
    "updated_at": "timestamp"
  },
